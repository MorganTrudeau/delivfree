{
  "name": "delivfreeapp",
  "version": "0.0.34",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "ios:consumer": "ENVFILE=.env.consumer react-native run-ios",
    "ios:vendor": "ENVFILE=.env.vendor react-native run-ios",
    "ios:admin": "ENVFILE=.env.admin react-native run-ios",
    "android:consumer": "ENVFILE=.env.consumer react-native run-android --active-arch-only",
    "android:vendor": "ENVFILE=.env.vendor react-native run-android --active-arch-only",
    "android:admin": "ENVFILE=.env.admin react-native run-android --active-arch-only",
    "compile": "tsc --noEmit -p . --pretty",
    "format": "prettier --config .prettierrc.js --write \"app/**/*.{js,jsx,json,md,ts,tsx}\"",
    "lint": "eslint index.js app test --fix --ext .js,.ts,.tsx && npm run format && tsc --noEmit",
    "ts": "tsc --noEmit",
    "patch": "patch-package",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:maestro": "maestro test .maestro/FavoritePodcast.yaml",
    "adb": "adb reverse tcp:9090 tcp:9090 && adb reverse tcp:3000 tcp:3000 && adb reverse tcp:9001 tcp:9001 && adb reverse tcp:8081 tcp:8081",
    "postinstall": "node ./bin/postInstall",
    "assemble:consumer": "cd android && ENVFILE=.env.consumer ./gradlew clean && ENVFILE=.env.consumer ./gradlew assembleRelease",
    "bundle:consumer": "cd android && ENVFILE=.env.consumer ./gradlew clean && ENVFILE=.env.consumer ./gradlew bundleRelease",
    "assemble:vendor": "cd android && ENVFILE=.env.vendor ./gradlew clean && ENVFILE=.env.vendor ./gradlew assembleRelease",
    "bundle:vendor": "cd android && ENVFILE=.env.vendor ./gradlew clean && ENVFILE=.env.vendor ./gradlew bundleRelease",
    "assemble:admin": "cd android && ENVFILE=.env.admin ./gradlew clean && ENVFILE=.env.admin ./gradlew assembleRelease",
    "bundle:admin": "cd android && ENVFILE=.env.admin ./gradlew clean && ENVFILE=.env.admin ./gradlew bundleRelease",
    "clean": "npx react-native-clean-project",
    "clean-all": "npx react-native clean-project-auto",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "build:consumer": "rm -rf dist/ && MODE=production ENVFILE=.env.consumer GENERATE_SOURCEMAP=false webpack --mode=production --config webpack.config.js",
    "build:vendor": "rm -rf dist/ && MODE=production ENVFILE=.env.vendor GENERATE_SOURCEMAP=false webpack --mode=production --config webpack.config.js",
    "build:admin": "rm -rf dist/ && MODE=production ENVFILE=.env.admin GENERATE_SOURCEMAP=false webpack --mode=production --config webpack.config.js",
    "host:consumer": "firebase deploy --only hosting:consumer",
    "host:vendor": "firebase deploy --only hosting:vendor",
    "host:admin": "firebase deploy --only hosting:admin",
    "web:consumer": "MODE=development ENVFILE=.env.consumer GENERATE_SOURCEMAP=false webpack serve --mode=development --config webpack.config.js",
    "web:vendor": "MODE=development ENVFILE=.env.vendor GENERATE_SOURCEMAP=false webpack serve --mode=development --config webpack.config.js",
    "web:admin": "MODE=development ENVFILE=.env.admin GENERATE_SOURCEMAP=false webpack serve --mode=development --config webpack.config.js"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "expo": {
    "autolinking": {
      "exclude": [
        "expo-application"
      ]
    }
  },
